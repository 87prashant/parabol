{
  "rewrites": [
    {
      "source": "/graphql",
      "destination": "https://action-staging.parabol.co/graphql"
    },
    {
      "source": "/sse/:path*",
      "destination": "https://action-staging.parabol.co/sse/:path*"
    },
    {
      "source": "/static/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/"
    }
  ]
}
